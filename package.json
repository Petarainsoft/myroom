{
  "name": "myroom-package",
  "version": "1.0.3",
  "private": false,
  "type": "module",
  "main": "./dist/myroom-system.umd.js",
  "module": "./dist/myroom-system.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "*.css",
    "./dist/*.css",
    "./dist/myroom-system.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/myroom-system.es.js",
      "require": "./dist/myroom-system.umd.js"
    },
    "./dist/myroom-system.css": "./dist/myroom-system.css",
    "./style.css": "./dist/myroom-system.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist || exit 0",
    "build": "echo 'Library already built. Please build in myroom-system.'"
  },
  "dependencies": {
    "@babylonjs/core": "^7.31.1",
    "@babylonjs/gui": "^7.31.1",
    "@babylonjs/loaders": "^7.31.1",
    "@babylonjs/materials": "^7.31.1",
    "@babylonjs/post-processes": "^7.31.1",
    "@types/lodash": "^4.17.20",
    "autoprefixer": "^10.4.21",
    "lodash": "^4.17.21",
    "lucide-react": "^0.526.0",
    "postcss": "^8.5.6",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "sonner": "^2.0.6",
    "tailwindcss": "^3.4.17"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}